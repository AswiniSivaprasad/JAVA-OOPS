package com.kodnest.Javaoops.SUPERKeyword;

class Vehicle {
	String type = "Generic Vehicle";
}

class Car extends Vehicle {
	String type = "Car";
	void printType() {
		System.out.println(super.type);
	}
}

public class AccessingShadowedField {

	public static void main(String[] args) {
		Car car = new Car();
		car.printType();
	}
}
